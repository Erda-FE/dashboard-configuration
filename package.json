{
  "name": "@terminus/dashboard-configurator",
  "version": "1.2.2",
  "description": "Dice Charts with Echart",
  "main": "./src/index.ts",
  "types": "./src/types/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://gittar.app.terminus.io/terminus-dice/dashboard-configurator"
  },
  "keywords": [
    "dice",
    "charts",
    "react",
    "Echarts"
  ],
  "sideEffects": [
    "*.scss",
    "*.css",
    "*.less",
    "./src/static/*.js"
  ],
  "scripts": {
    "adcode-map": "node tools/gen-adcode-map",
    "analyze": "rm -rf dist && NODE_ENV=production ./node_modules/.bin/webpack --config webpack.analyze.js",
    "build": "rm -rf dist && NODE_ENV=production ./node_modules/.bin/webpack",
    "changelog": "conventional-changelog -p angular -r 2 -i CHANGELOG.md -s",
    "commit": "./node_modules/cz-customizable/standalone.js",
    "dll": "./node_modules/.bin/webpack --config webpack_dll.config.js",
    "icon": "node tools/gen-icon",
    "lint-fix": "eslint --fix src ./webpack.config.js --ext '.js,.jsx,.tsx,.ts'",
    "prewatch": "shell/check-dll.sh",
    "preweb": "shell/check-dll.sh",
    "pub-alpha": "npm version prerelease --preid alpha && npm publish --tag alpha && git push",
    "pub-beta": "npm version prerelease --preid beta && npm publish --tag beta && git push",
    "release": "node ./tools/release.js",
    "stylelint-fix": "stylelint 'src/**/*.scss' --config .stylelintrc.json --fix",
    "watch": "NODE_ENV=development ./node_modules/.bin/webpack --watch",
    "web": "./node_modules/.bin/webpack serve --config webpack.server.js"
  },
  "author": "dice",
  "license": "ISC",
  "peerDependencies": {
    "react": ">=16.14.0",
    "react-dom": ">=16.14.0"
  },
  "dependencies": {
    "@terminus/nusi": "^3.10.78",
    "classnames": "^2.2.6",
    "cube-state": "1.2.1",
    "dom-to-image": "^2.6.0",
    "echarts": "^4.9.0",
    "echarts-for-react": "^2.0.16",
    "eslint-plugin-react-hooks": "^4.2.0",
    "git-revision-webpack-plugin": "^3.0.6",
    "immer": "^8.0.0",
    "lodash": "^4.17.20",
    "moment": "^2.29.1",
    "react-dnd": "^11.1.3",
    "react-dnd-html5-backend": "^11.1.3",
    "react-grid-layout": "^1.2.0",
    "react-use": "^15.3.8",
    "superagent": "^6.1.0",
    "superagent-use": "^0.1.0",
    "use-immer": "^0.4.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@terminus/lint-config": "^0.3.2",
    "@types/classnames": "^2.2.11",
    "@types/dom-to-image": "^2.6.2",
    "@types/echarts": "^4.9.3",
    "@types/lodash": "^4.14.167",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-grid-layout": "^1.1.1",
    "@types/react-redux": "^7.1.15",
    "@types/react-router": "^5.1.11",
    "@types/superagent": "^4.1.10",
    "@types/webpack": "^4.41.26",
    "@types/webpack-env": "^1.16.0",
    "@typescript-eslint/eslint-plugin": "^4.13.0",
    "@typescript-eslint/parser": "^4.13.0",
    "autoprefixer": "^10.2.1",
    "babel-eslint": "^10.1.0",
    "chalk": "^4.1.0",
    "conventional-changelog-cli": "^2.1.1",
    "copy-webpack-plugin": "^7.0.0",
    "css-loader": "^5.0.1",
    "css-minimizer-webpack-plugin": "^1.2.0",
    "cz-customizable": "^6.3.0",
    "eslint": "^7.17.0",
    "eslint-config-ali": "^11.4.1",
    "eslint-import-resolver-webpack": "^0.13.0",
    "eslint-plugin-compat": "^3.9.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-typescript": "^0.14.0",
    "html-webpack-plugin": "^5.0.0-beta.5",
    "husky": "^4.3.7",
    "inquirer": "^8.0.0",
    "less": "^4.1.0",
    "less-loader": "^7.2.1",
    "lint-staged": "^10.5.3",
    "mini-css-extract-plugin": "^1.3.4",
    "node-sass": "^5.0.0",
    "ora": "^5.3.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "regenerator-runtime": "^0.12.1",
    "rucksack-css": "^0.8.6",
    "sass-loader": "^10.1.1",
    "sass-resources-loader": "^2.1.1",
    "semver": "^7.3.4",
    "speed-measure-webpack-plugin": "^1.3.3",
    "style-loader": "^2.0.0",
    "stylelint": "^13.8.0",
    "stylelint-config-standard": "^20.0.0",
    "terser-webpack-plugin": "^5.1.1",
    "thread-loader": "^3.0.1",
    "ts-import-plugin": "^1.6.7",
    "ts-loader": "^8.0.14",
    "tsx-control-statements": "^4.0.0",
    "typescript": "^4.1.3",
    "webpack": "^5.14.0",
    "webpack-bundle-analyzer": "^4.3.0",
    "webpack-cli": "^4.3.1",
    "webpack-dev-server": "^3.11.2",
    "webpack-visualizer-plugin": "^0.1.11"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "eslint --fix",
      "git add"
    ],
    "src/**/*.tsx": [
      "eslint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -e $GIT_PARAMS"
    }
  },
  "files": [
    "src",
    "README.md",
    "package.json",
    "CHANGELOG.md"
  ]
}
