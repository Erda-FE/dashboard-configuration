{
  "name": "@terminus/dashboard-configurator",
  "version": "1.0.0-beta.2",
  "description": "Dice Charts with Echart",
  "main": "./src/index.ts",
  "types": "./src/types/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://gittar.app.terminus.io/terminus-dice/dashboard-configurator"
  },
  "keywords": [
    "dice",
    "charts",
    "react",
    "Echarts"
  ],
  "sideEffects": [
    "*.scss",
    "*.css",
    "*.less"
  ],
  "scripts": {
    "prewatch": "shell/check-dll.sh",
    "watch": "rm -rf dist && ENV=watch ./node_modules/.bin/webpack --progress --color -w -d",
    "preweb": "shell/check-dll.sh",
    "web": "webpack-dev-server --config webpack.server.js --mode development",
    "dll": "webpack --config webpack_dll.config.js --mode development",
    "icon": "node tools/gen-icon",
    "adcode-map": "node tools/gen-adcode-map",
    "build": "rm -rf dist && NODE_ENV=production ./node_modules/.bin/webpack --color",
    "analyze": "rm -rf dist && NODE_ENV=production ./node_modules/.bin/webpack --config webpack.analyze.js",
    "commit": "./node_modules/cz-customizable/standalone.js",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "precommit": "lint-staged",
    "lint-fix": "eslint --fix src ./webpack.config.js --ext '.js,.jsx,.tsx,.ts'",
    "stylelint-fix": "stylelint 'src/**/*.scss' --config .stylelintrc.json --fix",
    "pub": "npm version patch && npm publish && git push",
    "pub-alpha": "npm version prerelease --preid alpha && npm publish --tag alpha && git push",
    "pub-beta": "npm version prerelease --preid beta && npm publish --tag beta && git push"
  },
  "author": "terminus",
  "license": "ISC",
  "dependencies": {
    "@terminus/nusi": "^3.8.59",
    "@types/superagent": "^4.1.10",
    "classnames": "^2.2.6",
    "cube-state": "1.2.1",
    "dom-to-image": "^2.6.0",
    "echarts": "^4.2.1",
    "echarts-for-react": "^2.0.15-beta.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "immer": "^8.0.0",
    "lodash": "^4.17.14",
    "moment": "^2.27.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-grid-layout": "^1.0.0",
    "react-sizeme": "^2.5.2",
    "react-use": "^15.1.0",
    "screenfull": "^5.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@terminus/lint-config": "^0.3.2",
    "@types/classnames": "^2.2.3",
    "@types/dom-to-image": "^2.6.2",
    "@types/echarts": "^4.1.3",
    "@types/lodash": "^4.14.107",
    "@types/react": "^16.3.14",
    "@types/react-dom": "^16.0.5",
    "@types/react-grid-layout": "^0.16.7",
    "@types/react-redux": "^6.0.9",
    "@types/react-router": "^4.0.26",
    "@types/screenfull": "^4.1.0",
    "@types/webpack": "^4.4.0",
    "@types/webpack-env": "^1.15.3",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "autoprefixer": "^6.5.3",
    "babel-eslint": "^10.1.0",
    "copy-webpack-plugin": "^4.5.2",
    "css-loader": "^0.28.11",
    "cz-customizable": "^6.3.0",
    "eslint": "^7.13.0",
    "eslint-config-ali": "^11.2.0",
    "eslint-import-resolver-webpack": "^0.13.0",
    "eslint-plugin-compat": "^3.8.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-typescript": "^0.12.0",
    "happypack": "^5.0.0",
    "hard-source-webpack-plugin": "^0.13.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^4.3.0",
    "less": "^3.0.4",
    "less-loader": "^4.1.0",
    "lint-staged": "^7.2.2",
    "mini-css-extract-plugin": "0.4.1",
    "node-sass": "^4.9.0",
    "optimize-css-assets-webpack-plugin": "^5.0.0",
    "postcss-loader": "^1.1.1",
    "regenerator-runtime": "^0.12.1",
    "rucksack-css": "^0.8.6",
    "sass-loader": "^6.0.1",
    "sass-resources-loader": "^2.0.0",
    "speed-measure-webpack-plugin": "^1.3.3",
    "style-loader": "^0.13.2",
    "stylelint": "^13.7.2",
    "stylelint-config-standard": "^20.0.0",
    "superagent": "^6.0.0",
    "superagent-use": "^0.1.0",
    "svg-inline-loader": "^0.8.2",
    "ts-import-plugin": "^1.4.4",
    "ts-loader": "^4.5.0",
    "tsx-control-statements": "^4.0.0",
    "typescript": "^4.0.5",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "webpack": "^4.28.2",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-cli": "^3.1.1",
    "webpack-dev-server": "^3.1.4",
    "webpack-visualizer-plugin": "^0.1.11"
  },
  "lint-staged": {
    "example/**/*.ts": [
      "eslint --fix",
      "git add"
    ],
    "example/**/*.tsx": [
      "eslint --fix",
      "git add"
    ]
  },
  "files": [
    "src",
    "README.md",
    "package.json"
  ]
}
